{
  "name": "thomisticus/app-generator",
  "description": "Thomisticus App Generator",
  "keywords": [
    "laravel",
    "api",
    "model",
    "request",
    "migration",
    "model",
    "crud",
    "repository",
    "view",
    "test",
    "generator",
    "swagger"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Igor Moraes",
      "email": "igor.sgm@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/support": "^6.0.0",
    "laracasts/flash": "~3.0",
    "icanboogie/inflector": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "mockery/mockery": "~0.9",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Thomisticus\\Generator\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "\\Thomisticus\\Generator\\ThomisticusGeneratorServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "complete-cs-check": [
      "@check-cs-psr12"
    ],
    "complete-cs-fix": [
      "@fix-cs-psr12"
    ],
    "complete-test": [
      "@test-unit",
      "@test-coverage"
    ],
    "check-cs-psr2": "./vendor/bin/phpcs --standard=PSR2 --colors src/",
    "check-cs-psr12": "./vendor/bin/phpcs --standard=PSR12 --colors src/",
    "fix-cs-psr2": "./vendor/bin/phpcbf --standard=PSR2 --colors src/",
    "fix-cs-psr12": "./vendor/bin/phpcbf --standard=PSR12 --colors src/",
    "test-unit": "./vendor/bin/phpunit",
    "test-coverage": "./vendor/bin/phpunit --coverage-html storage/phpunit"
  }
}
