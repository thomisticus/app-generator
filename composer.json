{
  "name": "thomisticus/app-generator",
  "description": "Thomisticus App Generator",
  "keywords": [
    "laravel",
    "api",
    "model",
    "request",
    "migration",
    "model",
    "crud",
    "repository",
    "view",
    "test",
    "generator",
    "swagger"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Igor Moraes",
      "email": "igor.sgm@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:igorsgm/eloquent-nested-attributes.git"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/support": "^5.6|^6.0",
    "icanboogie/inflector": "^1.4",
    "igorsgm/eloquent-nested-attributes": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "mockery/mockery": "~1.0",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Thomisticus\\Generator\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "\\Thomisticus\\Generator\\ThomisticusGeneratorServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "phpcs": "./vendor/bin/phpcs --standard=PSR12 --colors src/",
    "phpcbf": "./vendor/bin/phpcbf --standard=PSR12 --colors src/",
    "test": "./vendor/bin/phpunit",
    "test-coverage": "./vendor/bin/phpunit --coverage-html storage/phpunit"
  }
}
