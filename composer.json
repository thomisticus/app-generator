{
  "name": "thomisticus/crud-generator",
  "description": "Thomisticus Crud Generator",
  "keywords": [
    "laravel",
    "api",
    "model",
    "request",
    "migration",
    "model",
    "crud",
    "repository",
    "view",
    "test",
    "generator",
    "swagger"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Igor Moraes",
      "email": "igor.sgm@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": ">=5.0.*",
    "prettus/l5-repository": "~2.6",
    "laracasts/flash": "~3.0",
    "icanboogie/inflector": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "mockery/mockery": "~0.9"
  },
  "autoload": {
    "psr-4": {
      "Thomisticus\\Generator\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "\\Thomisticus\\Generator\\ThomisticusGeneratorServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
